{
  "name": "gtfs-realtime-feed",
  "version": "0.1.0",
  "description": "App which retrieves gtfs realtime data from Transport for Ireland GTFS-R API",
  "main": "src/index.js",
  "dependencies": {
    "@hapi/hapi": "^20.0.3",
    "axios": "^0.20.0",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.0",
    "csv-parse": "^4.14.1",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.1",
    "gtfs-realtime-bindings": "0.0.6",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "mssql": "^6.2.3",
    "node-fetch": "^2.6.1",
    "pretty-error": "^2.1.2",
    "shelljs": "^0.8.4",
    "sqlstring": "^2.3.2",
    "strip-bom-stream": "^4.0.0",
    "tmp-promise": "^3.0.2",
    "untildify": "^4.0.0",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "sinon": "^9.2.1",
    "xo": "^0.36.1"
  },
  "scripts": {
    "start": "node .",
    "prod": "node . prod",
    "test": "xo && nyc mocha src/**/*.test.js",
    "import": "node ./src/import/gtfs-import"
  },
  "author": "Matthew O'Leary",
  "license": "MIT",
  "xo": {
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "no-unused-expressions": [
        "error",
        {
          "allowTernary": true
        }
      ],
      "eslint-comments/disable-enable-pair": [
        "off"
      ],
      "unicorn/import-style": [
        "error",
        {
          "styles": {
            "path": {
              "named": true
            }
          }
        }
      ],
      "unicorn/prevent-abbreviations": [
        "off"
      ]
    }
  }
}
